/* 轮播图 */
.nx_swiper swiper {
  width: 750rpx;
  height: 400rpx;
}
.nx_swiper swiper image {
  width: 750rpx;
  height: 400rpx;
}

/* banner列表 */
.nx_banner_list {
  margin: 20rpx;
  padding: 20rpx;
  background-color: white;
  line-height: 40rpx;
  border-radius: 10rpx;

}
.nx_banner_item {
  display: flex;
  color: #999;
}
.nx_banner_item_name {
  flex: 2;
}
.nx_banner_item_time {
  margin-left: 50rpx;
  flex: 1;
}
.nx_banner_item_arrow {
  flex: 1;
  text-align: right;
  color: #666;
}
/* 宫格列表 */
.nx_congar {
  margin: 20rpx;
}
.nx_congar_title {
  padding: 20rpx;
  color: #666;
  font-weight: bold;
  font-size: 35rpx;
  background-color: white;
  border-bottom: 1px solid #eee;
  border-radius: 10rpx 10rpx 0 0;
}
.input {
  padding: 20rpx;
  color: #666;
  font-weight: bold;
  font-size: 22rpx;
  background-color: white;
  border-bottom: 1px solid #eee;
  border-radius: 10rpx 10rpx 0 0;
}
.nx_congar_list {
  padding: 20rpx 20rpx 0 20rpx;
  display: flex;
  flex-wrap: wrap;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}
.nx_congar_list navigator {
  margin-bottom: 20rpx;
  text-align: center;
  color: #666;
}
.nx_item_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

/* 4宫格单独样式 */
.nx_congar_item_4 {
  width: 25%;
}
.nx_congar_item_4 image {
  width: 80%;
  height: 130rpx;
}
/* 3宫格单独样式 */
.nx_congar_item_3 {
  width: 33.33%;
}
.nx_congar_item_3 image {
  width: 90%;
  height: 180rpx;
}
/* 左大右2小单独样式 */
.nx_congar_list_1_2 {
  height: 350rpx;
  overflow: hidden;
  padding-bottom: 20rpx;
}
.nx_congar_item_left {
  flex: 2;
  height: 350rpx;
  padding-right: 20rpx;
}

.nx_congar_item_left image {
  height: 280rpx;
}
.nx_congar_item_right {
  flex: 1;
  height: 350rpx;
}
.nx_congar_item_right navigator {
  margin-bottom: 10rpx;
}
.nx_congar_item_right image {
  height: 113rpx;
}


