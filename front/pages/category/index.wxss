.container{
  height: 100%;
}

.container .container-left{
  height: 100%;
  border-right: 1px solid #c1c1c1;
  box-shadow: 3px 0 5px #c1c1c1;
  width: 100px;
  min-width: 100px;
}

.container .container-left .goodsType{
  background-color: white;
  padding: 5px;
  text-align: center;
  color: rgb(44, 41, 41);
}

.container .container-left .goodsTypeSelected{
  color: white;
  background-color: rgb(65,105,225);
}

.container .container-right{
  flex-grow: 1;
  height: 100%;
  align-items: center;
}

.book{
  width: 90%;
  padding: 10px;
  background: white;
  margin-top: 10px;
  align-items: center;
  border-radius: 10px;
  justify-content: center;
}

.book .name{
  font-size: 18px;
  font-weight: bold;
}

.book .imginfo{
  justify-content: center;
  margin-top: 10px;
  align-items: flex-end;
}

.book .imginfo .coverImg{
  width: 40px;
  height: 40px;
  border-radius: 6px;
  box-shadow: 3px 3px 8px #c1c1c1;
}

.book .imginfo .price{
  font-size: 14px;
  color: red;
  margin-left: 10px;
}

.book .des{
  width: 100%;
  margin-top:5px;
  font-size: 12px;
}

.book .des .left{
  min-width: 45px;
  width: 45px;
}

.book .des .right{
  color: black;
}

