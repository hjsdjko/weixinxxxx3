  flex-direction: column;
  justify-content: space-between;
  padding: 0 40rpx;
}
.nx_line_right {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #999;
}
.nx_line_left image {
  width: 100%;
  height: 100rpx;
}
.nx_line_left text {
  width: 80%;
  margin-left: 20rpx;
  color: #666;
}
.nx_line_item_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  font-weight: bold;
  font-size: 32rpx;
}

/* 图片 */
.nx_image {
  margin: 20rpx;
