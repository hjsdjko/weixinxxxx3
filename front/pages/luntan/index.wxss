}

/* 图片 */
.nx_image {
  margin: 20rpx;
  background-color: white;
}

.nx_congar_item_2 {
  width: 50%;
}
.nx_congar_item_2 image {
  width: 90%;
  height: 260rpx;
}
.face {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-top: 20px;
}

/* 搜索 */
.container-search{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 5px 10px;
}

.text-search{
  flex-grow: 1;
  margin-right: 15px;
  background: #fff;
  border: 1px solid #c1c1c1;
  border-radius: 6px;
  padding: 3px 6px;
  margin: 10px;
  font-size: 12px;
  text-align: left;
  color: #959595;
}

.searchIt{
  padding: 5px 10px;
  background: linear-gradient(to right, rgb(5, 177, 5),rgb(154, 248, 154));
  color: white;
  text-align: center;
  font-size: 12px;
  border-radius: 4px;
  box-shadow: 0 0 8px #c1c1c1;
}

/* 条目 */
.container-item{
  align-items: center;
  justify-content: center;
  margin: 0px 10px 10px 10px;
  box-shadow: 3px 3px 3px #c1c1c1;
  background-color: #fff;
  border-radius: 8px;
  padding: 10px;
  width: 90%;
}

.container-item .coverImage{
  border-radius: 8px;
}.container{
  align-items: center;
}

.book{
  
  width: 90%;
  padding: 10px;
  background: #fff;
  margin-top: 10px;
  align-items: left;
  border-radius: 10px;
  justify-content: center;
}
.book .image{
  position: absolute;
  width: 50%;
  height: 60rpx;
}
.book  .left{
  align-items: center;
 
}

.book  .left .selected{
  background: rgb(65,105,225);
  color: white;
  width: 20px;
  height: 20px;
  min-width: 20px;
  min-height: 20px;
  border-radius: 50%;
  text-align: center;
  border: 1px solid rgb(65,105,225);
}

.book  .left .unSelected{
  color: white;
  width: 20px;
  height: 20px;
  min-width: 20px;
  min-height: 20px;
  border-radius: 50%;
  text-align: center;
  border: 1px solid #c1c1c1;
} 
.book  .name{
  font-size: 18px;
  font-weight: bold;
  text-align: center;
}


.book .imginfo{
  text-align: center;
  margin-top: 10px;
  align-items: flex-end;
 
  
}


.book .imginfo .coverImg{
  width: 120px;
  height: 120px;
  border-radius: 6px;
  box-shadow: 3px 3px 8px #c1c1c1;
}
.book .imginfo .coverImg2{
  width: 200px;
  height: 80px;
  border-radius: 80px;
 
  overflow: hidden;
}
.book .imginfo .coverImg3{
  width: 50px;
  height: 50px;
  border-radius: 100px;
 
  overflow: hidden;
}

/* 新增 */
.book .imginfo .name{
  font-size: 18px;
  font-weight: bold;
}
.book .imginfo .name2{
  font-size: 15px;
  
}

.book .imginfo .zhihang{
  font-size: 14px;
  color: red;
  margin-left: 10px;
}

.book .des{
  width: 100%;
  margin-top:5px;
  margin-left:10px;
  font-size: 12px;
}

.book .des .left{
  min-width: 30px;
  width: 40px;
}

.book .des .right{
  color: black;
}

.container-bottom{
  height: 45px;
  align-items: center;
  justify-content: flex-end;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fff;
}

.container-bottom .add-cart{
  height: 45px;
  line-height: 45px;
  width: 110px;
  background-color: rgb(65,105,225);
  color: #fff;
  text-align: center;
}

.container-add{
  position: fixed;
  right: 0;
  bottom: 30px;
  padding: 14px;
  border-top-left-radius: 0%;
  border-bottom-left-radius: 0%;
  background:rgb(65,105,225);
  text-align: center;
  color: white;
}

.dianhua{
  height: 45px;
  line-height: 45px;
  width: 110px;
  background-color: rgb(65,105,225);
  color: #fff;
  text-align: left;
}
.book .des .mine-nouserinfo-avatar {
  position: absolute;
  left: 0; 
  background-color:transparent;
  
}
.book .des .mine-nouserinfo-avatar2 {
  display: flex;
  margin-right: auto;
  margin-left: auto; 
  background-color:transparent;
  
 
}
.book .des .mine-nouserinfo-avatar3 {
  position: absolute;
  right: 0; 
  background-color:transparent;
  border-top-left-radius: 100%;
  border-bottom-left-radius: 100%;
  
}
.mine-nouserinfo-avatar::after {
  border: none; 
}
.btnImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100px;
}
